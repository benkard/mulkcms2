{
  "dependencies": {
    "ContentTools": "^1.6.16",
    "bosonic": "^1.0.0",
    "elix": "^15.0.1",
    "normalize-opentype.css": "^0.2.4",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.29.0",
    "purecss": "^3.0.0",
    "sanitize.css": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "cssnano": "^5.1.13",
    "eslint": "^8.23.0",
    "flow-bin": "^0.233.0",
    "parcel": "^2.7.0",
    "snowpack": "2.8.0"
  },
  "browserslist": [
    "last 1 Firefox version",
    "last 1 iOS version",
    "last 1 Safari version",
    "last 1 Chrome version"
  ],
  "snowpack": {
    "plugins": [],
    "install": [
      "ContentTools/build/content-tools.min.css",
      "ContentTools/build/content-tools.min.js",
      "ContentTools/build/images/*",
      "bosonic",
      "elix/define/*.js",
      "elix/define/*.d.ts",
      "normalize-opentype.css/normalize-opentype.css",
      "normalize.css/normalize.css",
      "prismjs",
      "purecss/build/{base,buttons,forms,menus,tables}.css",
      "sanitize.css/{sanitize,forms,formsize,typography,page}.css"
    ]
  }
}
